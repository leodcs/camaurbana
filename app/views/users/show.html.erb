<article>
<h1><%= t '.title', user_name: @user.full_name %> </h1>

<ul>
	<li><%= t '.location', location: @user.location %></li>
	<li><%= t '.bio', bio: @user.bio %></li>
</ul>
<% if params[:id].to_i == current_user.id %>
	<%= link_to t('.edit'), edit_user_path(@user) %>
<% end %>
</article>